            "requires": "($new_container_title = 'fluffyalien_energistics:solar_panel')",
            "$screen_content": "fluffyalien_energistics:solar_panel.root",
            "$screen_bg_content": "common.screen_background"
          },
          {
            "requires": "($new_container_title = 'fluffyalien_energistics:crusher')",
            "$screen_content": "fluffyalien_energistics:crusher.root",
            "$screen_bg_content": "common.screen_background"
          }
        ]
      }
    ]
  }
}
